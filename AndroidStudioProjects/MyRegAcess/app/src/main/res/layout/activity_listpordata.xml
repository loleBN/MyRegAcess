<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tcc.lolebn.myregacess.telas.ListPorData">

    <LinearLayout
        android:layout_width="375dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/txt_pordata_data"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:text=""/>

            <Button
                android:id="@+id/btn_pordata_getdata"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="@dimen/nav_header_vertical_spacing"
                android:text="D" />
        </LinearLayout>

        <ListView
            android:id="@+id/lvPorData"
            android:layout_width="368dp"
            android:layout_height="match_parent"
            />

    </LinearLayout>




</android.support.constraint.ConstraintLayout>
